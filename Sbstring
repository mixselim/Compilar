//CPP
#include<iostream>
#include<cmath>
#include<cstdio>
#include<cstring>
using namespace std;
int main()
{
    int len,i,j,k;
    char a[100];
    gets(a);
    len = strlen(a);
    for(i=0;i<len;i++)
    {
        for(j=i;j<len;j++)
        {
            for(k=i;k<=j;k++)
            {
                cout<<a[k];
            }
            cout<<"\n";
        }
    }
    return 0;
}
