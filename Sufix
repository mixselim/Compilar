#include<stdio.h>
#include<string.h>
#include<ctype.h>
int main()
{
    char a[99999];
    int i,j,len;
    scanf("%s",a);
    len = strlen(a);
    for(i=len-1;i>=0;i--)
    {
        for(j=i;j<=len;j++)
        {
            printf("%c",a[j]);
        }
        printf("\n");
    }
    return 0;
}

